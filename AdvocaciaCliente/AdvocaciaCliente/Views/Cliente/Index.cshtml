@{

 }
<div id="divtitulo">
    <h1>Lista de Clientes</h1>
</div>
<div id="divtabela">
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">Código</th>
                <th scope="col">Nome</th>
                <th scope="col">Cidade</th>
                <th scope="col">Estado</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.listaClientes)
            {
            <tr>
                <td>@item.CLIID</td>
                <td>@item.CLINOME</td>
                <td>@item.CIDADE</td>
                <td>@item.ESTADOSIGLA</td>
            </tr>
            }
            <tr>
                <td>1</td>
                <td>Maria</td>
                <td>BaixaDaEgua</td>
                <td>SE</td>
            </tr>

<tbody>
  </table>
</div>
